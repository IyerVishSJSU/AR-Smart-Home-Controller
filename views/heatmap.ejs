<!DOCTYPE html>
<html>
  <head>
    <title>Heat Map</title>
    <meta name="Description" content="A website for home automation using Augmented Reality ">
    <meta name="Keywords" content="AR, Smart Home, webar,web,home,control,dashboard,datatables,devices,controller">

    <!--Import Google Icon Font-->
    <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>

    <!-- Stylesheet Library -->
    <link rel='stylesheet' href='/bower_components/materialize/dist/css/materialize.css' />

    <!-- Stylesheet Custom -->
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel='stylesheet' href='/stylesheets/heatmap.css' />
    <link rel='stylesheet' href="/bower_components/bootstrap/dist/css/bootstrap.min.css" />

    <!-- Script Library -->
    <script src="/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="/bower_components/materialize/dist/js/materialize.js"></script>
    <script src="/node_modules/collections/collections.min.js"></script>
    <script src="/bower_components/materialize/dist/js/materialize.js"></script>
    <script src="/bower_components/highcharts/highstock.js"></script>
    <script src="/bower_components/highcharts/highcharts.js"></script>
    <script src="/bower_components/highcharts/modules/heatmap.js"></script>
    <script src="/bower_components/highcharts/modules/treemap.js"></script>
    <script src="/bower_components/highcharts/modules/exporting.js"></script>
  </head>
  <body>
    <% include header %> <% include sidenav %>
    <div class="main">
      <div class="container">
          <div class="row">
            <div class="col l12">
              <div id="highchart1"></div>
              <div id="buttongroup">
                <p><strong>From: </strong><span id="from"></span>&nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp<strong>To: </strong><span id ="to"></span></p>
                <input type="button" class="btn btn-info" value="<<Prev Week" id="btnPrev"/> &nbsp &nbsp &nbsp
                <input type="button" class="btn btn-info" value="Next Week >>" id="btnNext"/>
                </div>    
            </div>
          </div>

          <div class="row">
            <div class="col l12">
            </div>
          </div>

          <div class="row">
            <div class="col l12">
            </div>
          </div>

          <div class="row">
            <div class="col l12">
              <div id="highchart2"></div>
            </div>
          </div>                     
      </div>
    </div>

  <% include footer %>

  <!-- Initialize Scripts -->  
  <script src="/javascripts/heatmap_config.js"></script>
  <script src="/javascripts/initialize.js"></script>
  </body>
</html>
